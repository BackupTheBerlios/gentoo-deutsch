depend() {
	need localmount
	use logger
}

start() {
	ebegin "Starting sasl pwcheck daemon"
	start-stop-daemon --start --quiet --oknodo --exec /usr/sbin/pwcheck
	eend $?
}

stop() {
	ebegin "Stopping sasl pwcheck daemon"
	start-stop-daemon --stop --quiet --oknodo --exec /usr/sbin/pwcheck
	eend $?
}
